				"source": [
					"import pandas as pd\r\n",
					"import requests\r\n",
					"class cowin():\r\n",
					"    def __init__(self,date,district):\r\n",
					"        self.date=date\r\n",
					"        self.district=district\r\n",
					"      \r\n",
					"    def cowin_data(self):\r\n",
					"        \r\n",
					"        dist_id=list(df1[df1['districts']==self.district]['district_id'])[0]\r\n",
					"        response=requests.get('https://cdn-api.co-vin.in/api/v2/appointment/sessions/public/findByDistrict?',params={'district_id':str(dist_id),'date':self.date})\r\n",
					"        c=[]\r\n",
					"        try:\r\n",
					"            for i in response.json():\r\n",
					"                for k in response.json()[i]:\r\n",
					"                    c.append(k)           \r\n",
					"        except:\r\n",
					"            pass\r\n",
					"        \r\n",
					"        return pd.DataFrame(c)\r\n",
					"\r\n",
					"cowin('01-09-2021','Kanchipuram').cowin_data()"
				],
